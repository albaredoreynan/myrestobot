<%= render :partial => "/account/setup_module/organization_setup/partials/container_header" %>
<div class="container">
<h2>Restaurant</h2>
<div class="well">
  <h3>Restaurant Information</h3>
  <dl class="dl-horizontal">
    <dt>Name</dt>
      <dd><%= @concept.name %></dd>
    <dt>Address</dt>
      <dd><%= @concept.address %></dd>
	  <dt>Contact Number</dt>
      <dd><%= @concept.contact_number %></dd>
  </dl>
</div>

<%= link_to "<i class='icon-chevron-left'></i> Back to Restaurant".html_safe, account_setup_module_organization_setup_concepts_path, :class => 'btn' %>
<%= link_to "<i class='icon-trash icon-white'></i> Delete".html_safe, account_setup_module_organization_setup_concept_path(@concept), :confirm => 'Are you sure?', :method => :delete, :class => 'btn btn-danger' %>
<%= link_to "<i class='icon-edit'></i> Edit".html_safe, edit_account_setup_module_organization_setup_concept_path, :class => 'btn' %>
</div>
<br/>
<%= render :partial => "/account/setup_module/organization_setup/partials/container_footer" %>