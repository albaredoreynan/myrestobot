<%= render :partial => "/account/setup_module/organization_setup/partials/container_header" %>
<div class="container">
<h2>Branch</h2>
<div class="well">
  <h3>Branch Information</h3>
    <dl class="dl-horizontal">
     <dt>Name</dt>
       <dd><%= @branch.name %></dd>
     <dt>Address</dt>
       <dd><%= @branch.address %></dd>
     <dt>Contact Number</dt>
       <dd><%= @branch.contact_number %></dd>
    </dl>
</div>

<%= link_to "<i class='icon-chevron-left'></i> Back to Branches".html_safe, account_setup_module_organization_setup_branches_path, :class => 'btn' %>
<%= link_to "<i class='icon-trash icon-white'></i> Delete".html_safe, account_setup_module_organization_setup_branch_path(@branch), :confirm => 'Are you sure?', :method => :delete, :class => 'btn btn-danger' %>
<%= link_to "<i class='icon-edit'></i> Edit".html_safe, edit_account_setup_module_organization_setup_branch_path, :class => 'btn' %>
</div>
<br/>

<%= render :partial => "/account/setup_module/organization_setup/partials/container_footer" %>